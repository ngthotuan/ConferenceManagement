<fx:root fx:id="root" prefHeight="105.0" prefWidth="620.0" stylesheets="@HoiNghiList.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/1.8.251" xmlns:fx="http://javafx.com/fxml/1">
